<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="EffectIcon">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:background">@null</item>
        <item name="android:layout_width">@dimen/effect_icon_size</item>
        <item name="android:layout_height">@dimen/effect_icon_size</item>
        <item name="android:scaleType">fitCenter</item>
    </style>
    <style name="EffectLabel">
        <item name="android:textSize">@dimen/effect_label_text_size</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">@dimen/effect_label_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/effect_label_margin_top</item>
        <item name="android:maxLines">1</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowRadius">2.0</item>
    </style>
    <style name="Effect">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingLeft">@dimen/effect_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/effect_padding_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="EffectsContainer">
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">@dimen/effects_container_padding</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="EffectsBar">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="EffectLabelInToolPanel" parent="@style/EffectLabel">
        <item name="android:layout_marginTop">0.0dip</item>
    </style>
    <style name="SeekBar">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:paddingLeft">@dimen/seekbar_padding_horizontal</item>
        <item name="android:paddingTop">@dimen/seekbar_padding_vertical</item>
        <item name="android:paddingRight">@dimen/seekbar_padding_horizontal</item>
        <item name="android:paddingBottom">@dimen/seekbar_padding_vertical</item>
        <item name="android:layout_width">@dimen/seekbar_width</item>
        <item name="android:layout_height">@dimen/seekbar_height</item>
        <item name="android:maxHeight">@dimen/seekbar_height</item>
        <item name="android:minHeight">@dimen/seekbar_height</item>
    </style>
    <style name="ActionBarInner">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">?android:actionBarSize</item>
    </style>
    <style name="ActionBarOuter">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ActionBarLinearLayout">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="ActionBarBackLinearLayout" parent="@style/ActionBarLinearLayout">
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="ActionBarImageView">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:adjustViewBounds">true</item>
    </style>
    <style name="ActionBarArrow">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/action_bar_arrow_margin_left</item>
        <item name="android:layout_marginRight">@dimen/action_bar_arrow_margin_right</item>
        <item name="android:src">?android:homeAsUpIndicator</item>
    </style>
    <style name="ActionBarIcon" parent="@style/ActionBarImageView">
        <item name="android:paddingLeft">@dimen/action_bar_icon_padding_left</item>
        <item name="android:paddingTop">@dimen/action_bar_icon_padding_vertical</item>
        <item name="android:paddingRight">@dimen/action_bar_icon_padding_right</item>
        <item name="android:paddingBottom">@dimen/action_bar_icon_padding_vertical</item>
        <item name="android:src">@mipmap/ic_launcher_icon</item>
    </style>
    <style name="ActionBarTitle" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="ImageActionButton" parent="@style/ActionBarImageView">
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:paddingLeft">@dimen/action_button_padding_horizontal</item>
        <item name="android:paddingTop">@dimen/action_button_padding_vertical</item>
        <item name="android:paddingRight">@dimen/action_button_padding_horizontal</item>
        <item name="android:paddingBottom">@dimen/action_button_padding_vertical</item>
    </style>
    <style name="TextActionButton" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Menu">
        <item name="android:gravity">center</item>
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:paddingLeft">@dimen/action_button_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/action_button_padding_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="EffectsMenuContainer" parent="@style/ActionBarLinearLayout">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">@dimen/effects_menu_container_width</item>
    </style>
    <style name="EffectsMenuActionButton" parent="@style/ImageActionButton">
        <item name="android:background">@drawable/photoeditor_toggle_button_background</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="FullscreenToolView">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="SpinnerProgressDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowTitleStyle">@null</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="Theme.Gallery" parent="@android:style/Theme.Holo">
        <item name="android:colorBackground">@null</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:actionBarStyle">@style/Holo.ActionBar</item>
        <item name="android:displayOptions"></item>
    </style>
    <style name="Theme.MovieActivity" parent="@style/Theme.Gallery">
        <item name="android:windowBackground">@android:color/black</item>
    </style>
    <style name="Holo.ActionBar" parent="@android:style/Widget.Holo.ActionBar">
        <item name="android:background">@drawable/actionbar_translucent</item>
        <item name="android:displayOptions">useLogo|showHome</item>
        <item name="android:backgroundStacked">@null</item>
    </style>
    <style name="MediaButton.Play" parent="@android:style/MediaButton.Play">
        <item name="android:background">@null</item>
        <item name="android:src">@drawable/icn_media_play</item>
    </style>
    <style name="DialogPickerTheme" parent="@style/Theme.Gallery" />
    <style name="Theme.Camera" parent="@android:style/Theme.Holo">
        <item name="android:colorBackground">@null</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:actionBarStyle">@style/Holo.ActionBar</item>
        <item name="android:displayOptions"></item>
    </style>
    <style name="Theme.Gallery.Camera" parent="@android:style/Theme.Holo">
        <item name="android:colorBackground">@null</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">2.0</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:actionBarStyle">@style/Holo.ActionBar</item>
        <item name="android:displayOptions"></item>
    </style>
    <style name="ThemeCamera" parent="@android:style/Theme.Holo.NoActionBar.Fullscreen">
        <item name="android:colorBackground">@android:color/black</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:colorBackgroundCacheHint">@android:color/black</item>
        <item name="android:displayOptions"></item>
    </style>
    <style name="OnScreenHintTextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@android:color/primary_text_dark</item>
        <item name="android:textColorHighlight">#ffff9200</item>
        <item name="android:textColorHint">#ff808080</item>
        <item name="android:textColorLink">#ff5c5cff</item>
    </style>
    <style name="OnScreenHintTextAppearance.Small" parent="@style/OnScreenHintTextAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@android:color/secondary_text_dark</item>
    </style>
    <style name="Animation_OnScreenHint">
        <item name="android:windowEnterAnimation">@anim/on_screen_hint_enter</item>
        <item name="android:windowExitAnimation">@anim/on_screen_hint_exit</item>
    </style>
    <style name="ReviewControlText">
        <item name="android:textSize">12.0sp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">2</item>
    </style>
    <style name="ReviewControlGroup">
        <item name="android:gravity">top|center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="PopupAnimation" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/popupwindow_in</item>
        <item name="android:windowExitAnimation">@anim/popupwindow_out</item>
    </style>
    <style name="ProgressBar_Large_new" parent="@android:style/Widget.ProgressBar">
        <item name="android:maxWidth">47.0dip</item>
        <item name="android:maxHeight">47.0dip</item>
        <item name="android:indeterminateDrawable">@drawable/progress_large_color</item>
        <item name="android:minWidth">47.0dip</item>
        <item name="android:minHeight">47.0dip</item>
    </style>
    <style name="ProgressTextStyle">
        <item name="android:textSize">7.0dip</item>
        <item name="android:typeface">sans</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="SettingRow">
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">@dimen/setting_item_list_margin</item>
        <item name="android:paddingRight">@dimen/setting_item_list_margin</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:minHeight">@dimen/setting_item_list_minheight</item>
    </style>
    <style name="SettingRowForPictureSize">
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">@dimen/picture_size_setting_item_list_margin_left</item>
        <item name="android:paddingRight">@dimen/picture_size_setting_item_list_margin_right</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:minHeight">@dimen/setting_item_list_minheight</item>
    </style>
    <style name="SeekBarStyle">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:paddingLeft">@dimen/seekbar_padding</item>
        <item name="android:paddingRight">@dimen/seekbar_padding</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:max">80</item>
        <item name="android:progressDrawable">@drawable/seekbar_img</item>
        <item name="android:thumb">@drawable/seekbar_thumb</item>
        <item name="android:thumbOffset">16.0dip</item>
    </style>
    <style name="ZoomBarStyle">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:paddingLeft">18.0dip</item>
        <item name="android:paddingRight">18.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:max">80</item>
        <item name="android:progress">0</item>
        <item name="android:progressDrawable">@drawable/bar_zoom_normal_land</item>
        <item name="android:thumb">@drawable/btn_controlpot_zoom_normal_land</item>
        <item name="android:thumbOffset">0.0dip</item>
    </style>
    <style name="ZoomBarStyleS10">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:paddingLeft">18.0dip</item>
        <item name="android:paddingRight">18.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:max">80</item>
        <item name="android:progress">0</item>
        <item name="android:progressDrawable">@drawable/bar_zoom_normal_land</item>
        <item name="android:thumb">@drawable/btn_controlpot_zoom_normal_land</item>
        <item name="android:thumbOffset">0.0dip</item>
    </style>
    <style name="SeekBarScaleStyle">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="PanoViewHorizontalBar">
        <item name="android:background">#ff000000</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="android:layout_weight">0.5</item>
        <item name="android:alpha">1.0</item>
    </style>
    <style name="progress_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:background">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="ViewfinderLableLayout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginLeft">13.0dip</item>
        <item name="android:layout_marginTop">13.0dip</item>
        <item name="android:layout_marginRight">13.0dip</item>
        <item name="android:layout_marginBottom">13.0dip</item>
    </style>
    <style name="OnViewfinderLabel">
        <item name="android:textSize">20.0dip</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:paddingTop">3.0dip</item>
        <item name="android:paddingRight">15.0dip</item>
        <item name="android:paddingBottom">3.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
</resources>
